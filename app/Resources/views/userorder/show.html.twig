{% extends 'base.html.twig' %}

{% block body %}
    <h1>Userorder</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ userOrder.id }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if userOrder.createdAt %}{{ userOrder.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{% if userOrder.status %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_orders_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_orders_edit', { 'id': userOrder.id }) }}">Edit</a>
        </li>
        {#<li>#}
            {#{{ form_start(delete_form) }}#}
                {#<input type="submit" value="Delete">#}
            {#{{ form_end(delete_form) }}#}
        {#</li>#}
    </ul>
{% endblock %}
